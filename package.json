{
  "name": "hane-theme-simple",
  "version": "0.2.0",
  "description": "A simple theme for hanejs.",
  "main": "lib/index.js",
  "scripts": {
    "eslint": "eslint --ext .es ./src",
    "watch": "babel src/ --watch --out-dir lib/",
    "build": "babel src/ --out-dir lib/ && lessc src/style.less > lib/style.css",
    "test:server": "node test/app.js",
    "test": "NODE_ENV=test mocha --recursive --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanejs/hane-theme-simple.git"
  },
  "keywords": [
    "hane",
    "hanejs",
    "theme",
    "react",
    "simple"
  ],
  "author": "hanejs <https://github.com/hanejs>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hanejs/hane-theme-simple/issues"
  },
  "homepage": "https://github.com/hanejs/hane-theme-simple#readme",
  "dependencies": {
    "html-beautify": "^1.0.4",
    "moment": "^2.18.1",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "sanitize-html": "^1.14.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2017-node7": "^0.4.1",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.11.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "faker": "^3.1.0",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.2.0",
    "react-hot-loader": "^1.3.1",
    "should": "^11.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.2"
  }
}
